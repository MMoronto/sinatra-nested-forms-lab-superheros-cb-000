<h1>Team and Heroes!</h1>

<div class="member">
  <h3>Hero Name: <%= @member.name %></h3><br>
  <h4>Hero Power: <%= @member.power %></h4><br>
  <h4>Hero Biography: <%= @member.bio %></h4>
</div><br>

<h1>Teams</h1>
<% @ships.each do |ship| %>
  <div class="ship">
    <p>Name: <%= ship.name %></p><br>
    <p>Type: <%= ship.type %></p><br>
    <p>Booty: <%= ship.booty %></p>
  </div><br>
<% end %>


